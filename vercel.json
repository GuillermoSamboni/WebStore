{
    "version": 2,
    "builds": [
        {
            "src": "dist/main.ts",
            "use": "@vercel/node",
            "config": {
                "maxDuration": 60
              }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "dist/main.ts",
            "methods": [
                "GET",
                "POST",
                "PUT",
                "DELETE"                
            ]
        }
    ]
}